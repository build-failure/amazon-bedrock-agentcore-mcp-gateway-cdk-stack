    "/cortex/complete": {
      "post": {
        "summary": "Generate text completion using Snowflake Cortex",
        "operationId": "cortexComplete",
        "parameters": [
          {
            "name": "Accept",
            "in": "header",
            "required": true,
            "description": "Content type that the client can accept",
            "schema": {
              "type": "string",
              "default": "application/json"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "model": {
                    "type": "string",
                    "description": "The model to use for completion",
                    "enum": [
                      "snowflake-arctic",
                      "llama2-7b-chat",
                      "llama2-13b-chat",
                      "llama2-70b-chat",
                      "mistral-7b",
                      "mistral-large",
                      "mixtral-8x7b",
                      "gemma-7b"
                    ]
                  },
                  "prompt": {
                    "type": "string",
                    "description": "The input prompt for text generation"
                  },
                  "options": {
                    "type": "object",
                    "properties": {
                      "max_tokens": {
                        "type": "integer",
                        "description": "Maximum number of tokens to generate",
                        "default": 512
                      },
                      "temperature": {
                        "type": "number",
                        "description": "Sampling temperature (0.0 to 1.0)",
                        "minimum": 0.0,
                        "maximum": 1.0,
                        "default": 0.7
                      },
                      "top_p": {
                        "type": "number",
                        "description": "Top-p sampling parameter",
                        "minimum": 0.0,
                        "maximum": 1.0,
                        "default": 1.0
                      }
                    }
                  }
                },
                "required": [
                  "model",
                  "prompt"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Text completion generated successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CortexCompletionResult"
                }
              }
            }
          },
          "400": {
            "description": "Invalid request parameters"
          }
        }
      }
    },
